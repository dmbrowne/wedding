{
  "name": "project-cupid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@zeit/next-sass": "^0.1.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "classnames": "^2.2.5",
    "cli-color": "^1.2.0",
    "connect-redis": "3.3.3",
    "express": "^4.16.2",
    "express-session": "1.15.6",
    "js-beautify": "^1.7.5",
    "lodash": "^4.17.5",
    "material-ui": "^1.0.0-beta.38",
    "next": "^5.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "sequelize": "^4.33.4",
    "uikit": "^3.0.0-beta.40"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "tsc || true && concurrently \"nodemon -w app/server index.js\" \"npm run tsc -- --watch\"",
    "test": "jest",
    "tsc": "tsc",
    "lint": "tslint **/*.ts",
    "build": "npm run clean && npm run tsc --",
    "create:migration": "node scripts/modelGenerate",
    "create:seed": "node scripts/seedGenerate",
    "migrate": "tsc -p migrations || true && sequelize db:migrate",
    "migrate:undo": "tsc -p migrations || true && sequelize db:migrate:undo",
    "migrate:undo:all": "tsc -p migrations || true && sequelize db:migrate:undo:all",
    "seed": "tsc -p migrations || true && sequelize db:seed",
    "seed:undo": "tsc -p migrations || true && sequelize db:seed:undo",
    "seed:undo:all": "tsc -p migrations || true && sequelize db:seed:undo:all"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/cli-color": "^0.3.29",
    "@types/connect-redis": "^0.0.7",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.1.3",
    "@types/js-beautify": "^0.0.31",
    "@types/lodash": "^4.14.105",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.38",
    "@types/sequelize": "types/sequelize",
    "concurrently": "^3.5.1",
    "rimraf": "^2.6.2",
    "sequelize-cli": "^4.0.0",
    "svg-to-react-cli": "^1.5.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  }
}
