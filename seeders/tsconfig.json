{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "allowJs": false,
    "sourceMap": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "types": [
      "node"
    ],
  },
  "filesGlob": [
    "typings/index.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "types": [
    "node"
  ],
  "compileOnSave": false
}